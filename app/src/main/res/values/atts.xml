<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Dialog">
        <attr name="DialogTitleAppearance" format="reference" />
        <attr name="DialogTitleText" format="reference|string" />
        <attr name="DialogSpotColor" format="reference|color" />
        <attr name="DialogSpotCount" format="integer" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MLImageView">
        <attr name="ml_border_color" format="color" />
        <attr name="ml_border_width" format="dimension" />
        <attr name="ml_press_alpha" format="integer" />
        <attr name="ml_press_color" format="color" />
        <attr name="ml_radius" format="dimension" />
        <attr name="ml_shape_type" format="enum">
            <enum name="none" value="0" />
            <enum name="round" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MyRadioButton">

        <attr name="my_drawableSize" format="dimension"/>
        <attr name="my_drawableTop" format="reference"/>
        <attr name="my_drawableLeft" format="reference"/>
        <attr name="my_drawableRight" format="reference"/>
        <attr name="my_drawableBottom" format="reference"/>

    </declare-styleable>

    <declare-styleable name="SectorMenuButton">
        <attr name="aebMainButtonSizeDp" format="dimension|reference" />
        <attr name="aebSubButtonSizeDp" format="dimension|reference" />
        <attr name="aebButtonGapDp" format="dimension|reference" />
        <attr name="aebStartAngleDegree" format="integer|reference" />
        <attr name="aebEndAngleDegree" format="integer|reference" />
        <attr name="aebAnimDurationMillis" format="integer|reference" />
        <attr name="aebButtonElevation" format="dimension|reference" />
        <attr name="aebMaskBackgroundColor" format="color|reference" />
        <attr name="aebIsSelectionMode" format="boolean" />
        <attr name="aebMainButtonTextColor" format="color|reference" />
        <attr name="aebSubButtonTextColor" format="color|reference" />
        <attr name="aebMainButtonTextSizeSp" format="dimension|reference" />
        <attr name="aebSubButtonTextSizeSp" format="dimension|reference" />
        <attr name="aebRippleEffect" format="boolean" />
        <attr name="aebRippleColor" format="color|reference"/>
        <attr name="aebMainButtonRotateDegree" format="integer|reference"/>
        <attr name="aebMainButtonRotateAnimDurationMillis" format="integer|reference"/>
        <attr name="aebBlurBackground" format="boolean" />
        <attr name="aebBlurRadius" format="float|reference"/>
    </declare-styleable>

    <declare-styleable name="QuickIndexBar">
        <attr name="font_color" format="color|reference"></attr>
        <attr name="selected_font_color" format="color|reference"></attr>
        <attr name="font_size" format="dimension|reference"></attr>
    </declare-styleable>

</resources>